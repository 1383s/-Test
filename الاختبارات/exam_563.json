                        },
                        {
                            "id": 48876,
                            "text": "توفير الأكسجين اللازم للتنفس الخلوي."
                        },
                        {
                            "id": 48877,
                            "text": "تحويل الجليكوجين إلى جلوكوز يتم أكسدته."
                        },
                        {
                            "id": 48878,
                            "text": "عبور الجلوكوز للأغشية البلازمية ليتم أكسدته داخل خلايا الجسم."
                        }
                    ]
                },
                {
                    "id": 12226,
                    "text": "أي مما يلي يميز الأنسولين عن الجلوكاجون؟",
